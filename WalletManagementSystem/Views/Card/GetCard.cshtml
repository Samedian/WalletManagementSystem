@model List<WalletManagementEntity.Card>; 
<table cellpadding="0" cellspacing="0" border="1" class="table-responsive">
    <tr>
        <th>Card Number</th>
        <th>Card Owner </th>
        <th>Expiry Date</th>
        <th>Delete</th>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>
                @item.CardNumber;

            </td>
            <td>
                @item.CardOwnerName
            </td>
            <td>
                @(item.Month +"/" + item.Year)
            </td>
        </tr>

    }
</table>

